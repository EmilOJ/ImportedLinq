<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Title>ImportedLinq</Title>
        <Description>ImportedLinq provides utility collection methods that are imported from other programming languages.</Description>
        <AssemblyName>ImportedLinq</AssemblyName>
        <Version>0.0.0.0</Version>
        <Authors>RyotaMurohoshi</Authors>
        <TargetFramework>netstandard1.0</TargetFramework>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/RyotaMurohoshi/ImportedLinq</PackageProjectUrl>
        <RepositoryUrl>https://github.com/RyotaMurohoshi/ImportedLinq</RepositoryUrl>
        <PackageTags>linq;extensions;collection</PackageTags>
        <Copyright>Copyright (c) 2019 RyotaMurohoshi</Copyright>
        <RepositoryType>git</RepositoryType>
        <LangVersion>latest</LangVersion>
        <PackageVersion>0.0.1.0</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="System.ValueTuple" Version="4.5.0" />
    </ItemGroup>

</Project>
